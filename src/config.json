{
    "j": "cmd:cmd_nextMsg",
    "k": "cmd:cmd_previousMsg",
    "o": "cmd:cmd_openConversation",
    "/": "cmd:cmd_showQuickFilterBar",
    "shift+/": "cmd:cmd_searchMessages",
    "g g": "window.document.querySelector('browser#mail3PaneTabBrowser1').contentDocument.querySelector('tbody[is=tree-view-table-body]').firstChild.click()",
    "shift+g": "window.document.querySelector('browser#mail3PaneTabBrowser1').contentDocument.querySelector('tbody[is=tree-view-table-body]').lastChild.click()",

    "alt+j": "$next-folder.js$",
    "alt+k": "$prev-folder.js$",
    "alt+d": "$next-mailbox.js$",
    "alt+u": "$prev-mailbox.js$",
    "g i": "$goto-inbox.js$",
    "g j": "$goto-junk.js$",
    "g d": "$goto-drafts.js$",
    "g s": "$goto-sent.js$",
    "g t": "$goto-trash.js$",

    "f": "cmd:cmd_forward",
    "r": "cmd:cmd_reply",
    "shift+r": "cmd:cmd_replyall",
    "shift+j": "cmd:cmd_markAsJunk",
    "shift+n": "cmd:cmd_markAsNotJunk",
    "x": "cmd:cmd_archive",
    "shift+x": "cmd:cmd_delete",

    "shift+a": "func:MsgNewMessage",
    "ctrl+l": "window.document.querySelector('browser#mail3PaneTabBrowser1').contentDocument.querySelector('#folderPaneGetMessages').click()"
}
